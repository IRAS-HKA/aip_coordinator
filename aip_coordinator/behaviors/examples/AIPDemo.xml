<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Repeat num_cycles="10">
            <Sequence>
                <Action ID="MoveArmToJoints" joint1="1.5" joint2="0" joint3="0.7" joint4="0"
                    joint5="0" joint6="0" />
                <Action ID="MoveArmToJoints" joint1="0" joint2="0" joint3="0" joint4="0" joint5="0"
                    joint6="0" />
            </Sequence>
        </Repeat>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="MoveArmToJoints">
            <input_port name="joint1" type="float" />
            <input_port name="joint2" type="float" />
            <input_port name="joint3" type="float" />
            <input_port name="joint4" type="float" />
            <input_port name="joint5" type="float" />
            <input_port name="joint6" type="float" />
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>